{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "rootDir": "./",
      "outDir": "./build",
      "esModuleInterop": true,
      "strict": true,

    "baseUrl" : ".",
    "paths" : {
      "@dataset/*" : ["dataset/*"],
      "@action/*" : ["src/action/*"],
      "@constant/*" : ["src/constant/*"],
      "@entity/*" : ["src/entity/*"],
      "@utils/*" : ["src/utils/*"]
    }
    }
}